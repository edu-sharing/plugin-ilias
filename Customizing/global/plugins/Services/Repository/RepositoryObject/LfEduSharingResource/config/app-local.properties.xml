<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<!-- comment describing this application-config-file -->
	<comment>My edu-sharing.net repository</comment>

	<!-- the application-id, used to identify in edu-sharing.network -->
	<entry key="appid">repository.stable.demo.edu-sharing.net</entry>
	
	<!-- a more verbose description of this application -->
	<entry key="appcaption">my repository manages learn-objects</entry>
	
	<!-- says if application is allowd to act like a client -->
	<entry key="trustedclient">true</entry>

	<!-- the type, one of LMS,REPOSITORY,SERVICE -->
	<entry key="type">REPOSITORY</entry>

	<!-- host: 	client: trusted host 
				server: access url  -->
	<entry key="host">127.0.0.1</entry>
	<entry key="port">80</entry>
	
	<entry key="contenturl">http://127.0.0.1:8080/edu-sharing/renderingproxy</entry>
	
	<!-- url's for the Authentication Web Service -->
	<entry key="authenticationwebservice">http://127.0.0.1:8080/edu-sharing/services/authbyapp</entry>
	<entry key="authenticationwebservice_wsdl">http://127.0.0.1:8080/edu-sharing/services/authbyapp?wsdl</entry>
	
	<!-- url's for the Usage Web Service -->
	<entry key="usagewebservice">http://127.0.0.1:8080/edu-sharing/services/usage2</entry>
	<entry key="usagewebservice_wsdl">http://127.0.0.1:8080/edu-sharing/services/usage2?wsdl</entry>

	<!-- base-url for repositorie's native alfresco-webservices -->
	<entry key="alfresco_webservice_url">http://127.0.0.1:8080/alfresco/api</entry>


</properties>
